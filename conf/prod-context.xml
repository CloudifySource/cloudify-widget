<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <bean id="deployManager" class="beans.DeployManager"/>
    <bean id="expiresServersCollector" class="beans.ExpireServersCollector"/>
    <bean id="serverBootstrapper" class="beans.ServerBootstrapper"/>
    <bean id="serverPool" class="beans.ServerPool"/>
    <bean id="widgetServer" class="beans.WidgetServer"/>

    <context:annotation-config />
    <!--<context:component-scan base-package="beans" />-->

</beans>